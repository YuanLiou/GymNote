-- Methods
getWorkoutById:
SELECT * FROM Workout WHERE id = :id;

getWorkoutByCategoryId:
SELECT * FROM Workout WHERE categoryId = :categoryId;

updateInitalCreateDate:
UPDATE Workout
SET createAt = ?, lastModified = ?
WHERE createAt IS NULL AND lastModified IS NULL;



