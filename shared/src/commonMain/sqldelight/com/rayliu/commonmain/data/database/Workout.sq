CREATE TABLE Workout (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    categoryId INTEGER NOT NULL,
    sportRecordTypeId INTEGER NOT NULL,
    createAt TEXT,
    lastModified TEXT
);

