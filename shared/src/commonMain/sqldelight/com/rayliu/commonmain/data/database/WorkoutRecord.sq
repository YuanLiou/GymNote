CREATE TABLE WorkoutRecord (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    workoutId INTEGER NOT NULL,
    createAt Text NOT NULL
);

-- Methods
insertNewRecord:
INSERT OR REPLACE
INTO WorkoutRecord
VALUES (?, ?, ?);

getWorkoutRecords:
SELECT *
FROM WorkoutRecord
WHERE id = :id
ORDER BY createAt DESC;

deleteWorkoutRecord:
DELETE FROM WorkoutRecord
WHERE id = :id;